/* 公共样式 */
.pop-up{
	border: 1px solid #ccc;
	border-radius: 5px;
	width: 20%;
	text-align: center;
	font-size: 10px;
	line-height: 30px;
	padding-top: 15px;
	position: fixed;
	top: 30%;
	left: 40%;
	background-color: #fff;
	display: none;
}
.pop-up p{
	font-size: 14px;
	color: green;
}

form .form-group{
	position: relative;
	/* border-bottom: 2rem !important; */
}
form .form-group .invalid-feedback{
	position: absolute;
	bottom: -20px;
}